<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="查询"
            visible="true"
            id="newpmbe.search.actionSet">
         <menu
               label="查询"
               id="searchMenu">
            <separator
                  name="searchGroup">
            </separator>
         </menu>
         <action
               label="&amp;活动查询..."

               class="newpmbe.search.actions.SearchActivityAction"

               menubarPath="searchMenu/searchGroup"

               id="newpmbe.search.actions.SampleAction">
         </action>
         <action
               label="&amp;产品查询..."

               class="newpmbe.search.actions.SearchProductAction"

               menubarPath="searchMenu/searchGroup"

               id="newpmbe.search.actions.SampleAction">
         </action>
      </actionSet>
   </extension>

</plugin>
